/* box shadow*/
body, .box:after{
    background:#E6E7E8;
}
 
.box, .content{
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
}
 
.box {
    background: white;
    position:relative;
    width: 1040px;
    height:450px;
    padding:15px;
    margin: 0 auto;
    margin-top: 0px;
}
 
.content{
    background:purple;
    width:100%;
    height:100%;
}
 
.shadow:before, .box:after{
    content: '';   
    position: absolute;
}
 
.shadow:before{
    background: #BEBEC0;
    height: 15px;
    width: 96%;
    bottom: -15px;
    left: 2%;
}
 
.shadow:after{
    -moz-border-radius: 150% / 20px;
    -webkit-border-radius: 150% / 20px;
    border-radius: 150% / 20px;
    width: 100%;
    height: 20px;
    bottom: -22px;
    left: 0;
    z-index: 1;
}

